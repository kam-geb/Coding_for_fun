    "import turtle\n",
    "import colorsys"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "t = turtle.Turtle()\n",
    "s = turtle.Screen()\n",
    "s.bgcolor('black')\n",
    "t.speed(0)\n",
    "n = 150\n",
    "h = 0\n",
    "t.left(100)\n",
    "\n",
    "for i in range(360):\n",
    "    c = colorsys.hsv_to_rgb(h, 1, 0.8)\n",
    "    h+= 1/n\n",
    "    t.color(c)\n",
    "    t.left(144)\n",
    "    t.forward(i*5)"
   ]
